{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm -w -F @ai-vibe/web build",
  "devCommand": "pnpm -C apps/web dev",
  "installCommand": "corepack enable && corepack prepare pnpm@9.15.0 --activate && pnpm install --network-timeout 600000",
  "framework": "nextjs",
  "outputDirectory": "apps/web/.next",
  "env": {
    "NODE_ENV": "production",
    "npm_config_fetch_retries": "10",
    "npm_config_fetch_retry_mintimeout": "30000",
    "npm_config_fetch_retry_maxtimeout": "180000",
    "npm_config_network_timeout": "600000"
  }
}
